		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5"
	},
	"dependencies": {
		"child-process": "^1.0.2",
		"concurrently": "^9.1.2",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"node-fetch": "^3.3.2",
		"proxy": "^2.2.0"
	}
}
